{
  "name": "hackwestx-functions",
  "scripts": {
    "build": "tsc",
    "serve": "yarn build && DEBUG=dev,info,error NODE_ENV=dev yarn start",
    "start": "node dist/index.js",
    "exec": "docker exec -it hackwestx-web-app_app_1 sh",
    "debug": "nodemon inspect --require ts-node/register src/index.ts",
    "test": "jest test",
    "coverage": "jest --collectCoverageFrom=src/**.ts --coverage test"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "google-auth-library": "^1.6.1",
    "googleapis": "27",
    "socket.io": "^2.1.1",
    "supports-color": "^5.4.0",
    "ts-node": "^7.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.0",
    "@types/socket.io": "^1.4.36",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "jest": "^23.4.1",
    "sinon": "^6.1.3",
    "socket.io-client": "^2.1.1",
    "ts-jest": "^23.0.1",
    "typescript": "^2.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "private": true
}
